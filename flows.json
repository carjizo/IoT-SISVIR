    {
        "id": "bc09de75de2f092d",
        "type": "postgresql",
        "z": "c7d83dd76c9dcb21",
        "name": "SISVIR_ID:5",
        "query": "DO $$\nBEGIN\n    IF EXISTS (SELECT mac FROM sisvir_dev.dispositivos WHERE mac = '{{ msg.payload.id }}') then\n        IF EXISTS (select paciente_id from sisvir_dev.dispositivos d where d.mac = '{{ msg.payload.id }}') then\n            IF EXISTS (SELECT * FROM sisvir_dev.signos_vitales WHERE paciente_id = (SELECT paciente_id FROM sisvir_dev.dispositivos WHERE mac = '{{ msg.payload.id }}')) then\n                UPDATE sisvir_dev.signos_vitales SET created_date = now() - INTERVAL '5 hours', pulso_cardiaco = {{ msg.payload.pulso }}, \n                temperatura = {{ msg.payload.temperatura }}\n                WHERE paciente_id = (SELECT paciente_id FROM sisvir_dev.dispositivos WHERE mac = '{{ msg.payload.id }}');\n            ELSE\n                INSERT INTO sisvir_dev.signos_vitales (created_date, pulso_cardiaco, temperatura, paciente_id) \n                VALUES (now() - INTERVAL '5 hours', {{ msg.payload.pulso }}, {{ msg.payload.temperatura }}, (SELECT paciente_id FROM sisvir_dev.dispositivos WHERE mac = '{{ msg.payload.id }}'));\n            END IF;\n        END IF;\n    END IF;\nEND $$;\n",
        "postgreSQLConfig": "1046833424e00bcc",
        "split": true,
        "rowsPerMsg": 1,
        "outputs": 1,
        "x": 430,
        "y": 240,
